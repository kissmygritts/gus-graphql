{
  "name": "gus-graphql",
  "version": "0.3.0",
  "description": "A graphql api for GUS",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -e js,json,graphql --exec babel-node src/index.js",
    "lint": "eslint src -c .eslintrc.json --ext js",
    "check-staged": "lint-staged",
    "test": "DB_URI_TEST=postgresql://localhost/gus_test jest",
    "test-server": "DB_URI_TEST=postgresql://localhost/gus_test npm start"
  },
  "author": "Mitchell Gritts",
  "license": "Unlicense",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^5.5.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-prettier-standard": "^2.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.0.0-rc.14",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.2",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "apollo-server": "^2.0.7",
    "apollo-server-express": "^2.0.6",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "graphql": "^14.0.2",
    "graphql-resolvers": "^0.2.2",
    "jsonwebtoken": "^8.3.0",
    "merge-graphql-schemas": "^1.5.4",
    "pg-monitor": "^1.0.0",
    "pg-promise": "^8.4.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
